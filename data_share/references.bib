@article{DevelopmentTestingWaterGAP_alcamo_2003,
  title = {Development and Testing of the {{WaterGAP}} 2 Global Model of Water Use and Availability},
  author = {ALCAMO, {\relax JOSEPH} and D{\"O}LL, {\relax PETRA} and HENRICHS, {\relax THOMAS} and KASPAR, {\relax FRANK} and LEHNER, {\relax BERNHARD} and R{\"O}SCH, {\relax THOMAS} and SIEBERT, {\relax STEFAN}},
