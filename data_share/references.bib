@article{BenchmarkingDatadrivenRainfall_lees_2021,
  title = {Benchmarking Data-Driven Rainfall\textendash Runoff Models in {{Great Britain}}: A Comparison of Long Short-Term Memory ({{LSTM}})-Based Models with Four Lumped Conceptual Models},
  shorttitle = {Benchmarking Data-Driven Rainfall\textendash Runoff Models in {{Great Britain}}},
  author = {Lees, Thomas and Buechel, Marcus and Anderson, Bailey and Slater, Louise and Reece, Steven and Coxon, Gemma and Dadson, Simon J.},
  year = {2021},
  month = oct,
  journal = {Hydrology and Earth System Sciences},
  volume = {25},
  number = {10},
  pages = {5517--5534},
  publisher = {{Copernicus GmbH}},
  issn = {1027-5606},
  doi = {10.5194/hess-25-5517-2021},
  urldate = {2023-04-03},
  abstract = {Long short-term memory (LSTM) models are recurrent neural networks from the field of deep learning (DL) which have shown promise for time series modelling, especially in conditions when data are abundant. Previous studies have demonstrated the applicability of LSTM-based models for rainfall\textendash runoff modelling; however, LSTMs have not been tested on catchments in Great Britain (GB). Moreover, opportunities exist to use spatial and seasonal patterns in model performances to improve our understanding of hydrological processes and to examine the advantages and disadvantages of LSTM-based models for hydrological simulation. By training two LSTM architectures across a large sample of 669 catchments in GB, we demonstrate that the LSTM and the Entity Aware LSTM (EA LSTM) models simulate discharge with median Nash\textendash Sutcliffe efficiency (NSE) scores of 0.88 and 0.86 respectively. We find that the LSTM-based models outperform a suite of benchmark conceptual models, suggesting an opportunity to use additional data to refine conceptual models. In summary, the LSTM-based models show the largest performance improvements in the north-east of Scotland and in south-east of England. The south-east of England remained difficult to model, however, in part due to the inability of the LSTMs configured in this study to learn groundwater processes, human abstractions and complex percolation properties from the hydro-meteorological variables typically employed for hydrological modelling.},
  langid = {english},
  file = {C:\Users\lei\Zotero\storage\WK3XHVQ8\Lees et al_2021_Benchmarking data-driven rainfall–runoff models.pdf}
}

@article{bhadraDevelopmentGeomorphologicalInstantaneous2008,
  title = {Development of a Geomorphological Instantaneous Unit Hydrograph Model for Scantily Gauged Watersheds},
  author = {Bhadra, A. and Panigrahy, N. and Singh, R. and Raghuwanshi, N. S. and Mal, B. C. and Tripathi, M. P.},
  year = {2008},
  month = aug,
  journal = {Environmental Modelling \& Software},
  volume = {23},
  number = {8},
  pages = {1013--1025},
  issn = {1364-8152},
  doi = {10.1016/j.envsoft.2007.08.008},
  urldate = {2022-09-14},
  abstract = {In recent years, geomorphological instantaneous unit hydrograph (GIUH) approach to estimate the runoff from a watershed due to different storm patterns has gained wide acceptability for watersheds with scanty data. In this study, a user-friendly event based computerized GIUH model, GIUH\_CAL, was developed. GIUH\_CAL model incorporates an infiltration component. Depending on the availability of soil and infiltration data, the model offers a choice among three methods of infiltration calculation, viz., Richards' equation, phi-index method and Philip two term model. The graphical user interface (GUI) of the model is based on mouse-driven approach with pop-up windows, pull-down menus, sub-menus, toolbar, statusbar and button controls including comprehensive context-sensitive help file. GIUH\_CAL model displays generated instantaneous unit hydrograph (IUH), unit hydrograph (UH) and direct runoff hydrograph (DRH) of the watershed graphically and processes statistically. Modelling efficiency (ME), coefficient of residual mass (CRM), root mean square error (RMSE) and coefficient of determination (r2) are used as the performance criteria. The model was tested for two distinctly different watersheds, i.e., a 4th order Guptamani watershed in tropical sub-humid region and a 3rd order Upper Chhokeranala watershed in semi-arid region of India. The results show that model performs satisfactorily for both test watersheds and performance is much better with Richards' equation option of infiltration calculation as compared to phi-index method and Philip two term model.Thus, GIUH\_CAL model can be adopted as a standard tool for modelling rainfall\textendash runoff transformation process in scantily gauged watersheds.},
  langid = {english},
  keywords = {Geomorphological instantaneous unit hydrograph (GIUH),Graphical user interface (GUI),Modeling,Ungauged watershed},
  file = {C:\Users\lei\Zotero\storage\KKQ8L6LM\S1364815207002034.html}
}

