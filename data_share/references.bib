@article{AquaCropOSOpenSource_foster_2017,
  title = {{{AquaCrop-OS}}: {{An}} Open Source Version of {{FAO}}'s Crop Water Productivity Model},
  shorttitle = {{{AquaCrop-OS}}},
  author = {Foster, T. and Brozovi{\'c}, N. and Butler, A. P. and Neale, C. M. U. and Raes, D. and Steduto, P. and Fereres, E. and Hsiao, T. C.},
  year = {2017},
