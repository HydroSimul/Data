@article{VariationsGlobalContinental_mullerschmied_2016,
  title = {Variations of Global and Continental Water Balance Components as Impacted by Climate Forcing Uncertainty and Human Water Use},
  author = {M{\"u}ller Schmied, Hannes and Adam, Linda and Eisner, Stephanie and Fink, Gabriel and Fl{\"o}rke, Martina and Kim, Hyungjun and Oki, Taikan and Portmann, Felix Theodor and Reinecke, Robert and Riedel, Claudia and Song, Qi and Zhang, Jing and D{\"o}ll, Petra},
  year = {2016},
