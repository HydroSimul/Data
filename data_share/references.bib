@article{WaterGAPGroundwater_doll_2014,
  title = {Global-Scale Assessment of Groundwater Depletion and Related Groundwater Abstractions: {{Combining}} Hydrological Modeling with Information from Well Observations and {{GRACE}} Satellites},
  shorttitle = {Global-Scale Assessment of Groundwater Depletion and Related Groundwater Abstractions},
  author = {D{\"o}ll, Petra and M{\"u}ller Schmied, Hannes and Schuh, Carina and Portmann, Felix T. and Eicker, Annette},
